def primediv(n):
    primes = [i for i in range(1, abs(n) + 1)]
    primes[0] = 0
    k = 1
    while k <= abs(n):
        if primes[k - 1] != 0:
            p = 2 * k
            while p <= abs(n):
                primes[p - 1] = 0
                p += k
        k += 1
    primes = list(filter(lambda a: a != 0 and n % a == 0, primes))
    print(max(primes))
